      "jsx-a11y/anchor-is-valid": ["warn", { "aspects": ["invalidHref"] }]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "ajv": "^6.5.4",
    "node-sass": "^4.9.3",
    "redux-devtools-extension": "^2.13.5"
  }
